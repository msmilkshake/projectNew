<table class="table table-striped">
  <tr>
    <th>id</th>
    <th>username</th>
    <th>email</th>
    <th>joined</th>
    <th>last updated</th>
  </tr>
  <% users.forEach(user=> { %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.uname %></td>
      <td><%= user.email %></td>
      <td><%= user.createdAt %></td>
      <td><%= user.updatedAt %></td>
    </tr>
  <% }) %>
<table>