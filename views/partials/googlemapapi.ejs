<div id="map" style="height: 50vh;"></div>
<script>
  var map;
  function initMap() {
    map = new google.maps.Map(document.getElementById('map'), {
      center: {lat: 27.7172453, lng: 85.3239605},
      zoom: 19,
      disableDoubleClickZoom: true
    });
  }
</script>
<script defer async src="/js/googleapi.js"></script>
<script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA_v3-uFrJWvGaafcCyVtRfYfo7d6k4pPg&callback=initMap"></script>
